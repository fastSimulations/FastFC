FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      fvSchemes;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

ddtSchemes
{
    default         steadyState;
}

gradSchemes
{
    default         Gauss linear;
}

divSchemes
{
    default         none;
}

laplacianSchemes
{
	laplacian(condElectronEff,potElectron)			Gauss harmonic corrected;  
	laplacian(condProtonEff,potProton)				Gauss harmonic corrected;
	laplacian(transportCoeffO2,wO2)					Gauss harmonic corrected;
	laplacian(transportCoeffH2OVap,wH2OVap)			Gauss harmonic corrected;
	laplacian(transportCoeffEnergy,T)				Gauss harmonic corrected;
	laplacian(dynamicPerm,pMix)						Gauss harmonic corrected;
	laplacian(diffH2OLiq,sLiq)						Gauss harmonic corrected;
}

interpolationSchemes
{
    default         linear;
}

snGradSchemes
{
    default         corrected;
}

fluxRequired
{
    default         yes;
}


