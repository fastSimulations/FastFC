FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      potElectron;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [  1  2 -3  0  0 -1  0];

internalField   uniform 0;

boundaryField
{
	wall_l 
	{
		type    zeroGradient;
	}

	wall_r
	{
		type	zeroGradient;
	}
	
   	wall_t
	{
       	type	zeroGradient;
	}

	wall_b
	{
		type	zeroGradient;
	}

	wall_empty
	{
		type	empty;
	}
	
}

